    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^6.0.0",
    "prettier": "^1.10.2"
  },
  "dependencies": {
    "growly": "^1.3.0",
    "semver": "^5.4.1",
    "shellwords": "^0.1.1",
    "which": "^1.3.0"
